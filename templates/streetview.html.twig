{% block content %}
<div id="street-view"></div>
<script>
    var panorama;
    function initialize(){
        panorama= new google.maps.StreetViewPanorama(
            document.getElementById('street-view'),
            {
                position: {lat: 36.800399, lng: 10.186619500000006},
                pov: {heading: 165, pitch: 0},
                zoom:1
            }
        );
    }
</script>
<script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDTdxaUb10x75S1mZehuzEAjEu5rzTFxCo&callback=initialize">
</script>
